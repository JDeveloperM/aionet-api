{
  "version": 2,
  "name": "aionet-backend",
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"]
}
